  <h1 style="color:#6D4F56;"><b>Create a Listing</b> </h1>

<%= simple_form_for @listing do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :estimated_hours %>
  <%= f.input :contact %>
  <%= f.association :skills, as: :check_boxes %>
  <%= f.button :submit, :class => 'btn-primary' %>
<% end %>
